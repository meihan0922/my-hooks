    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "eslint \"{packages,apps}/**/*.{js,jsx,ts,tsx}\"",
    "spellcheck": "cspell lint --dot --gitignore --color --cache --show-suggestions \"(packages|apps)/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md)\"",
    "commit": "git-cz",
    "prepare": "husky",
    "test": "pnpm --filter=./packages/* test",
    "test:coverage": "pnpm --filter=./packages/* test:coverage"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write --parser=typescript"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meihan0922/my-hooks.git"
  },
