  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "commitizen": "4.3.1",
    "commitlint": "20.3.1",
    "cspell": "^9.6.0",
    "cz-git": "1.12.0",
    "eslint": "9.39.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "17.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "turbo": "2.7.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
